{
  "name": "cat-energy-18",
  "version": "17.0.0",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^9.7.5",
    "browser-sync": "^2.26.7",
    "gulp": "^4.0.2",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "pixel-glass": "^1.0.13",
    "stylelint": "13.2.x",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.sass\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp css",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.16"
  }
}
